mysql> SELECT title_id,title,type, 
    -> CASE substr(title_id, 1, 2) 
    -> 
    -> WHEN 'BU' THEN 'Business'
    -> WHEN 'MC' THEN 'Modern Cooking'
    -> WHEN 'PC' THEN 'Popular Computers'
    -> WHEN 'PS' THEN 'Psychology'
    -> WHEN 'TC' THEN 'Traditional Cooking'
    -> END 
    -> "type"
    -> FROM titles;
